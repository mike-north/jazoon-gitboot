<h1>{{org.name}} Repositories</h1>
<ul class='repos'>
  {{#each model as |repo index|}}
    <li>
      <i class="material-icons" onclick={{action 'repoFavorited' repo}}>
        star_border
      </i>
      {{#link-to 'org.repo' repo.id}}
        {{repo.name}}
      {{/link-to}}
      ({{nice-number repo.stargazersCount}})
    </li>
  {{/each}}
</ul>
{{outlet}}
